
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id                            String                       @id @default(auto()) @map("_id") @db.ObjectId
  email                         String                       @unique
 
}

model DataVisualizationDashboard {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  Day       String
  Age       String
  Gender    String
  A         Int
  B         Int
  C         Int
  D         Int
  E         Int
  F         Int
}

